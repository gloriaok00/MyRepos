<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace绑定一个对应的Dao/Mapper接口-->
<mapper namespace="com.example.demo.mbs.lianxi.mb3.UserDao">
    <!--Select查询语句-->
    <select id="getUserList" resultType="com.example.demo.mbs.lianxi.mb3.User">
        select * from u_user
    </select>
</mapper>
