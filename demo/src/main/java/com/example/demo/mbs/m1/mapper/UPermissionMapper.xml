        <result column="action" property="action" />
        <result column="remark" property="remark" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        create_time,
        id, url, name, pid, type_id, path, action, remark
    </sql>

</mapper>
